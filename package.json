{
  "name": "pos",
  "version": "2.3.3",
  "description": "A billing software for creating and managing invoices and customer accounts.",
  "main": "./out/main/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Prajwal-17/pos"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noemit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noemit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "postinstall": "electron-builder install-app-deps",
    "dev": "pnpm run rebuild:electron && cross-env ELECTRON_DISABLE_SANDBOX=1 electron-vite dev",
    "rebuild:node": "pnpm rebuild better-sqlite3",
    "rebuild:electron": "electron-rebuild -f -w better-sqlite3",
    "db:seed": "pnpm run rebuild:node && tsx src/main/db/seed.ts",
    "migrate": "pnpm run rebuild:node && drizzle-kit migrate",
    "studio": "pnpm run rebuild:node && drizzle-kit studio",
    "push": "pnpm run rebuild:node && drizzle-kit push",
    "build": "npm run typecheck && electron-vite build",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win --publish never",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux --publish never"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/dotenv": "^8.2.3",
    "better-sqlite3": "^12.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.3",
    "drizzle-seed": "^0.3.1",
    "googleapis": "^159.0.0",
    "jspdf": "^3.0.2",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.534.0",
    "motion": "^12.23.12",
    "next-themes": "^0.4.6",
    "react-day-picker": "^9.9.0",
    "react-hot-toast": "^2.6.0",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^7.7.1",
    "react-to-print": "^3.1.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.6",
    "uuid": "^11.1.0",
    "zod": "^4.1.8",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/rebuild": "^4.0.1",
    "@faker-js/faker": "^9.9.0",
    "@tanstack/eslint-plugin-query": "^5.91.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.16.5",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.7.0",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.31.4",
    "electron": "^31.0.1",
    "electron-builder": "^25.1.8",
    "electron-updater": "^6.6.2",
    "electron-vite": "^4.0.0",
    "eslint": "^9.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-scan": "^0.4.3",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.5"
  },
  "build": {
    "appId": "pos.quickcart",
    "productName": "QuickCart",
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "artifactName": "${productName}-${version}.${ext}"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "Prajwal-17",
        "repo": "pos"
      }
    ]
  },
  "linux": {
    "target": [
      "AppImage"
    ],
    "category": "Office"
  }
}